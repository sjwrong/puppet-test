<VirtualHost *:80>
 ServerName <%= @vhost_domain %>
 DocumentRoot <%= @vhost_root %>
 ErrorLog /var/log/apache2/<%= @vhost_domain %>-error_log
 CustomLog /var/log/apache2/<%= @vhost_domain %>-access_log
  common

 <Directory <%= @vhost_root%>>
  Allow from all
  Options +Includes +Indexes +FollowSymLinks
  AllowOverride all

 </Directory>
</VirtualHost>
